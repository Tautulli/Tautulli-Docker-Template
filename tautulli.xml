<?xml version="1.0"?>
<Container version="2">
  <Name>tautulli</Name>
  <Repository>tautulli/tautulli</Repository>
  <Registry>https://registry.hub.docker.com/u/tautulli/tautulli/</Registry>
  <Network>br0</Network>
  <MyIP/>
  <Privileged>false</Privileged>
  <Support>https://discord.gg/tQcWEUp</Support>
  <Project>http://tautulli.com/</Project>
  <Overview>Tautulli is a 3rd party application that you can run alongside your Plex Media Server to monitor activity and track various statistics.&#xD;
&#xD;
This is the official container supported by the Tautulli devs.</Overview>
  <Category>Tools: MediaServer:Other</Category>
  <WebUI>http://[IP]:[PORT:8181]/</WebUI>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/Tautulli/Tautulli-Docker/master/img/logo-circle.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <DateInstalled>1520708215</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <DonateImg/>
  <MinVer/>
  <Description>Tautulli is a 3rd party application that you can run alongside your Plex Media Server to monitor activity and track various statistics.&#xD;
&#xD;
This is the official container supported by the Tautulli devs.</Description>
  <Networking>
    <Mode>br0</Mode>
    <Publish>
      <Port>
        <HostPort>8181</HostPort>
        <ContainerPort/>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir/>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir/>
      <ContainerDir>/logs</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>99</Value>
      <Name>PUID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>PGID</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Config Name="Config" Target="/config" Default="/mnt/cache/appdata/tautulli/" Mode="rw" Description="Config location" Type="Path" Display="always" Required="true" Mask="false"/>
  <Config Name="Plex Logs" Target="/logs" Default="" Mode="rw" Description="Path to Plex logs" Type="Path" Display="always" Required="false" Mask="false"/>
  <Config Name="PUID" Target="PUID" Default="99" Mode="" Description="PUID" Type="Variable" Display="always" Required="false" Mask="false">99</Config>
  <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="PGID" Type="Variable" Display="always" Required="false" Mask="false">100</Config>
  <Config Name="HTTP Port" Target="" Default="8181" Mode="tcp" Description="Tautulli HTTP port" Type="Port" Display="always" Required="false" Mask="false">8181</Config>
</Container>
